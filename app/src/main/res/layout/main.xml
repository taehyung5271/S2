<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ddf7e3"
    tools:context=".Main"
    android:id="@+id/main_ac"
    >
    <FrameLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/f1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"/>
    <ImageView
        android:layout_width="30sp"
        android:layout_height="30sp"
        android:layout_marginTop="28dp"
        android:src="@drawable/s2"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:layout_width="30sp"
        android:layout_height="30sp"
        android:layout_marginLeft="324dp"
        android:layout_marginTop="28dp"
        android:src="@drawable/bell"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:id="@+id/main_al"
        />

    <ImageView
        android:id="@+id/shop"
        android:layout_width="30sp"
        android:layout_height="30sp"
        android:layout_marginLeft="368dp"
        android:layout_marginTop="28dp"
        android:src="@drawable/locker"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <ImageView
        android:layout_width="30sp"
        android:layout_height="30sp"
        android:layout_marginLeft="64dp"
        android:layout_marginTop="28dp"
        android:src="@drawable/placeholder"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:id="@+id/main_gps"
        />


    <ImageView
        android:layout_width="30sp"
        android:layout_height="30sp"
        android:layout_marginLeft="15sp"
        android:layout_marginTop="28dp"
        android:src="@drawable/rabbit"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:id="@+id/main_rb"
        />

    <TextView
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginLeft="24sp"
        android:layout_marginTop="38sp"
        android:gravity="center"
        android:text="33"
        android:textColor="#5D9C59"
        android:textSize="10sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="312dp"
        android:layout_marginTop="64dp"
        android:gravity="center"
        android:text="철수 ♥ 나미"
        android:textColor="#5D9C59"
        android:textSize="15sp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="300dp"
        android:layout_marginTop="84dp"
        android:gravity="center"
        android:text="사랑한지 N일째"
        android:textColor="#5D9C59"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:layout_width="30sp"
        android:layout_height="30sp"
        android:layout_marginLeft="364dp"
        android:layout_marginTop="116dp"
        android:src="@drawable/camera"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:id="@+id/main_p"
        />

    <ImageView
        android:layout_width="185dp"
        android:layout_height="192dp"
        android:layout_marginTop="148dp"
        android:background="@drawable/shape1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="329dp"
        android:layout_height="128dp"
        android:layout_marginTop="360dp"
        android:background="@drawable/shape"
        android:gravity="center"
        android:text="다마고치 말풍선"
        android:textColor="#5D9C59"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="153sp"
        android:layout_marginTop="520dp"
        android:background="@color/white"
        android:gravity="center"
        android:text="질문지"
        android:textColor="#5D9C59"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottom"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        app:itemIconTint="#006400"
        app:itemTextColor="#006400"
        app:layout_constraintBottom_toBottomOf="parent"
        app:menu="@menu/bottom_nav_menu"/>
</androidx.constraintlayout.widget.ConstraintLayout>